{
    "name": "watershed-ui",
    "version": "0.0.1",
    "description": "A UI to visualize NA watersheds.",
    "main": "/src/app.jsx",
    "scripts": {
        "start": "npm run clean && webpack-dev-server --config ./build-config/webpack.dev.config.js",
        "build": "npm run clean && webpack --config ./build-config/webpack.config.js",
        "deploy": "npm run build && gh-pages -d build",
        "clean": "rm -Rf ./build/bundle.js ./build/index.html",
        "reset": "npm run clean && rm -Rf ./node_modules ./package-lock.json"
    },
    "keywords": [
        "visualization",
        "watershed",
        "react",
        "redux",
        "d3"
    ],
    "author": "hci-lab",
    "license": "MIT",
    "dependencies": {
        "core-js": "^3.1.4",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-redux": "^7.1.0",
        "react-router": "^3.2.3",
        "react-router-dom": "^5.0.1",
        "redux": "^4.0.2",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.5.0",
        "@babel/preset-env": "^7.5.0",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.3.0",
        "css-loader": "^6.7.3",
        "gh-pages": "^2.0.1",
        "html-loader": "^4.2.0",
        "html-webpack-harddisk-plugin": "^1.0.1",
        "html-webpack-plugin": "^5.5.0",
        "node-sass": "^4.12.0",
        "redux-devtools-extension": "^2.13.8",
        "sass-loader": "^13.2.0",
        "style-loader": "^3.3.1",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^3.7.2"
    }
}
